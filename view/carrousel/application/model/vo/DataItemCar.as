package com.digitalarbor.toyota.view.carrousel.application.model.vo {
	import flash.display.Sprite;
	
	/**
	 * @author denis.gonzalez
	 */
	public class DataItemCar extends Sprite {
		
		private var _url_imagen_up : String;
		private var _url_imagen_over : String;
		private var _url_imagen_down : String;
		private var _title : String;
		private var _url_link : String;
		private var _idItemcar : String;		
		private var _isEnable : String;
		private var _filterAnswers:Array;
		
		public function DataItemCar() {
		
		}
		
		public function get url_imagen_up() : String {
			return _url_imagen_up;
		}
		
		public function set url_imagen_up(url_imagen_up : String) : void {
			_url_imagen_up = url_imagen_up;
		}
		
		public function get url_imagen_over() : String {
			return _url_imagen_over;
		}
		
		public function set url_imagen_over(url_imagen_over : String) : void {
			_url_imagen_over = url_imagen_over;
		}
		
		public function get url_imagen_down() : String {
			return _url_imagen_down;
		}
		
		public function set url_imagen_down(url_imagen_down : String) : void {
			_url_imagen_down = url_imagen_down;
		}
		
		public function get url_link() : String {
			return _url_link;
		}
		
		public function set url_link(url_link : String) : void {
			_url_link = url_link;
		}
		
		public function get isEnable() : String {
			return _isEnable;
		}
		
		public function set isEnable(isEnable : String) : void {
			_isEnable = isEnable;
		}
		
		public function get title() : String {
			return _title;
		}
		
		public function set title(title : String) : void {
			_title = title;
		}
		
		public function get idItemcar() : String {
			return _idItemcar;
		}
		
		public function set idItemcar(idItemcar : String) : void {
			_idItemcar = idItemcar;
		}
		
		//
		public function get filterAnswers():Array { return _filterAnswers; }
		
		public function set filterAnswers(value:Array):void 
		{
			_filterAnswers = value;
		}
	}
}
